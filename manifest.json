{
  "name": "Minute for Minute",
  "version": "1.0",
  "description": "An extension to help with focus during study or work time. On installation, please open the settings page to instill default values/change the configuration to your needs.",
  "content_security_policy": "script-src 'self' ; object-src 'self'",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "options_page": "settings.html",
  "background": {
    "scripts": ["timer.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
